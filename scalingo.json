{
  "build": {
    "docker": true
  },
  "env": {
    "ALWAYS_DEPLOY_LATEST": "True",
    "APP_ID": "<your_app_id>",
    "API_HASH": "<your_api_hash>",
    "BOT_TOKEN": "<your_bot_token>",
    "OWNER": "<your_owner_id>",
    "LOG_CHANNEL": "<your_log_channel_id>",
    "WORKERS": "1",
    "FCHANNEL": "<your_forward_channel_id>",
    "FCHANNEL_STAT": "<your_forward_channel_status_message_id>",
    "FBANNER": "True",
    "FSTICKER": "<your_forward_channel_sticker_divider>",
    "CAP_DECO": "ðŸŒ¸",
    "DUMP_LEECH": "False",
    "CACHE_DL": "True",
    "DATABASE_URL": "<your_mongodb_database_url>",
    "FFMPEG": "<your_ffmpeg_command>"
  },
  "addons": [
    {
      "plan": "scalingo-mongodb:free",
      "options": {
        "version": "<desired_mongodb_version>"
      }
    }
    // Add any other addons here
  ]
}
